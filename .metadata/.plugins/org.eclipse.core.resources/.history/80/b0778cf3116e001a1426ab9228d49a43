public class Test {

	public static void main(String[] args) 
	{
		String raw = "";
		try 
		{
			raw = BibleParser.parseChapter(BibieID.KJV, BookID.REV, 100);
		} 
		catch (UnsupportedEncodingException e) 
		{
			e.printStackTrace();
		}
		System.out.println (raw);
	}

}
