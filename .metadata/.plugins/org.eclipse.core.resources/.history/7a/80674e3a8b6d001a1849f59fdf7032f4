package constants;

public enum BibieID 
{
	DRA ("DRA", "179568874c45066f-01"),
	FBVNTPsalms ("FBVNTPsalms", "65eec8e0b60e656b-01"),
	GNV ("GNV", "c315fa9f71d4af3a-01"),
	KJV ("KVJ", "de4e12af7f28f599-01");
	
	private String kBibleID;
	private String kBibleKey;
	private String kTranslationName;
	
	private BibieID (String bibleID, String bibleKey)
	{
		this.kBibleID = bibleID;
		this.kBibleKey = bibleKey;
	}
}
